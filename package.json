{
  "name": "css-less",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development yarn cache clean && yarn css && ../web/node_modules/.bin/next dev & yarn style:watch",
    "build": "NODE_ENV=production yarn css && ../web/node_modules/.bin/next build",
    "start": "../web/node_modules/.bin/next start",
    "restart": "yarn build && yarn start",
    "style": "node ../less-watcher-compiler --gulpfile ../less-watcher-compiler/gulpfile.js --cwd ./",
    "style:watch": "yarn ../web/style --sourcemap",
    "css": "yarn ../web/style symlink && yarn style css"
  },
  "peerDependencies": {
    "less-watcher-compiler": "0.1.x"
  },
  "devDependencies": {
    "@zeit/next-css": "^1.0.1",
    "chalk": "^4.1.0",
    "next": "9.3.5",
    "next-redux-wrapper": "^6.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^3.3.0",
    "semantic-ui-less": "^2.4.1"
  }
}
